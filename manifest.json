{
  "manifest_version": 2,
  "name": "dor1an 2FA",
  "version": "0.1.1",
  "description": "A Chrome extension for dor1an2FA",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "popup.html",
    "js/popup.js"
  ],
  "browser_action": {
    "default_popup": "setup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self';"
}






