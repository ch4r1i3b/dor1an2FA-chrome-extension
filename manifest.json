{
  "manifest_version": 2,
  "name": "dor1an 2FA",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "popup.html",
    "js/popup.js"
  ],
  "browser_action": {
    "default_popup": "configure.html"
  }
}






